<?xml version="1.0"?>
<odoo>
    <record id="crm_activity_log_view_form" model="ir.ui.view">
        <field name="name">Log an Activity Wolftrak</field>
        <field name="model">crm.activity.log</field>
        <field name="inherit_id" ref="crm.crm_activity_log_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form/group/group" position="inside">
                <field name="call_duration" widget="float_time"/>
            </xpath>
        </field>
    </record>
</odoo>